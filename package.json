{
  "name": "@travi/ioc",
  "license": "MIT",
  "description": "extremely simple container that loosely aligns to IOC",
  "version": "0.0.0-semantically-released",
  "main": "lib/ioc.cjs.js",
  "module": "lib/ioc.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s lint:* tests:*",
    "build": "rollup -c",
    "preversion": "npm test",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "presemantic-release": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": "travi/ioc",
  "bugs": "https://github.com/travi/ioc/issues",
  "homepage": "https://github.com/travi/ioc#readme",
  "keywords": [
    "ioc",
    "inversion-of-control"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "devDependencies": {
    "@travi/any": "1.4.3",
    "@travi/eslint-config-travi": "1.5.19",
    "babel-cli": "6.26.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "chai": "4.1.2",
    "cz-conventional-changelog": "2.1.0",
    "husky": "0.15.0-beta.2",
    "mocha": "4.0.1",
    "npm-run-all": "4.1.1",
    "rollup": "0.51.5",
    "rollup-plugin-babel": "3.0.2",
    "semantic-release": "8.2.0",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
