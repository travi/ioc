{
  "name": "@travi/ioc",
  "license": "MIT",
  "description": "extremely simple container that loosely aligns to IOC",
  "version": "0.0.0-semantically-released",
  "main": "lib/ioc.cjs.js",
  "module": "lib/ioc.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s lint:* tests:*",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepack": "npm run build",
    "commitmsg": "commitlint -e",
    "precommit": "npm test"
  },
  "repository": "travi/ioc",
  "bugs": "https://github.com/travi/ioc/issues",
  "homepage": "https://github.com/travi/ioc#readme",
  "keywords": [
    "ioc",
    "inversion-of-control"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "devDependencies": {
    "@babel/register": "^7.4.4",
    "@travi/any": "2.0.7",
    "@travi/babel-preset": "^3.0.0",
    "@travi/eslint-config-travi": "1.8.7",
    "babel-register": "6.26.0",
    "chai": "^4.2.0",
    "commitlint-config-travi": "1.3.4",
    "cz-conventional-changelog": "3.0.2",
    "husky": "^4.0.2",
    "mocha": "6.2.2",
    "npm-run-all": "4.1.5",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "4.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
