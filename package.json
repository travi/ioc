{
  "name": "@travi/ioc",
  "license": "MIT",
  "description": "extremely simple container that loosely aligns to IOC",
  "version": "0.0.0-semantically-released",
  "main": "lib/ioc.cjs.js",
  "module": "lib/ioc.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "mocha 'src/**/*-test.js'",
    "test": "npm-run-all --print-label --parallel lint:* test:*",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "repository": "travi/ioc",
  "bugs": "https://github.com/travi/ioc/issues",
  "homepage": "https://github.com/travi/ioc#readme",
  "keywords": [
    "ioc",
    "inversion-of-control"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "devDependencies": {
    "@babel/register": "7.22.5",
    "@rollup/plugin-babel": "6.0.3",
    "@travi/any": "2.1.8",
    "@travi/babel-preset": "3.0.105",
    "@travi/eslint-config": "4.0.2",
    "@travi/eslint-config-mocha": "1.0.20",
    "babel-register": "6.26.0",
    "chai": "4.3.7",
    "commitlint-config-travi": "1.4.35",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "rollup": "3.28.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
