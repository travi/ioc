{
  "name": "@travi/ioc",
  "description": "extremely simple container that loosely aligns to IOC",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/ioc",
  "bugs": "https://github.com/travi/ioc/issues",
  "homepage": "https://github.com/travi/ioc#readme",
  "keywords": [
    "ioc",
    "inversion-of-control"
  ],
  "main": "lib/ioc.cjs.js",
  "module": "lib/ioc.es.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "lint:js": "eslint .",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "mocha 'src/**/*-test.js'",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "prebuild": "run-s clean",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepack": "npm run build",
    "prepare": "husky install",
    "lint:publish": "publint"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.28.3",
    "@rollup/plugin-babel": "6.1.0",
    "@travi/any": "3.1.3",
    "@travi/babel-preset": "3.0.146",
    "@travi/eslint-config": "5.0.12",
    "@travi/eslint-config-mocha": "1.0.25",
    "babel-register": "6.26.0",
    "chai": "6.2.1",
    "commitlint-config-travi": "1.4.59",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "mocha": "11.7.5",
    "npm-run-all2": "8.0.4",
    "publint": "0.3.15",
    "rimraf": "6.1.0",
    "rollup": "4.53.3"
  }
}
