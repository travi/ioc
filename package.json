{
  "name": "@travi/ioc",
  "license": "MIT",
  "description": "extremely simple container that loosely aligns to IOC",
  "version": "0.0.0-semantically-released",
  "main": "lib/ioc.cjs.js",
  "module": "lib/ioc.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "lint:peer": "npm ls >/dev/null",
    "tests:unit": "mocha 'src/**/*-test.js'",
    "test": "run-s lint:* tests:*",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepack": "npm run build"
  },
  "repository": "travi/ioc",
  "bugs": "https://github.com/travi/ioc/issues",
  "homepage": "https://github.com/travi/ioc#readme",
  "keywords": [
    "ioc",
    "inversion-of-control"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "devDependencies": {
    "@babel/register": "7.15.3",
    "@travi/any": "2.0.18",
    "@travi/babel-preset": "3.0.67",
    "@travi/eslint-config": "1.0.84",
    "@travi/eslint-config-mocha": "1.0.12",
    "babel-register": "6.26.0",
    "chai": "4.3.4",
    "commitlint-config-travi": "1.4.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "mocha": "9.1.3",
    "npm-run-all": "4.1.5",
    "rollup": "2.58.1",
    "rollup-plugin-babel": "4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
